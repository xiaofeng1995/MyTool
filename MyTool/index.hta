	<link rel="icon" href="./favicon.ico" type="image/x-icon">
	<HTA:Application
		applicationName="MyTools"
		icon="favicon.ico"
		border="thin"
		caption="yes"
		maximizebutton="no"
		minimizebutton="no"
		sysmenu="no"
		windowstate="normal"
	>
	<script type="text/javascript">
		var htaWidth=800;//hta宽度
		var htaHeight=550;//hta高度
		var windowHeight=window.screen.availHeight;//屏幕高度
		var windowWidth=window.screen.availWidth;//屏幕高度
		window.onload=function(){
		  window.moveTo((windowWidth-htaWidth)/2,(windowHeight-htaHeight)/2);//移动hta窗体位置在屏幕剧�?
		  window.resizeTo(htaWidth,htaHeight);//更改hta窗体大小
		}
	</script>
</head>
